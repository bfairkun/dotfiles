{
    "[markdown][plaintext][log]": {
        "editor.quickSuggestions": {
          "comments": "off",
          "strings": "off",
          "other": "off"
        },
      },
    "[plaintext]": {
      "editor.quickSuggestions": {
        "comments": "off",
        "strings": "off",
        "other": "off"
      }
    },
    "[markdown]": {
      "editor.quickSuggestions": {
        "comments": "off",
        "strings": "off",
        "other": "off"
      }
    },
    "[log]": {
      "editor.quickSuggestions": {
        "comments": "off",
        "strings": "off",
        "other": "off"
      }
    },
    "workbench.colorTheme": "Default Light Modern",
    "r.plot.useHttpgd": true,
    "r.alwaysUseActiveTerminal": true,
    "r.lsp.diagnostics": false,
    "r.session.data.pageSize": 15,
    "editor.minimap.enabled": false,
    "r.session.data.rowLimit": 5000,
    "editor.hover.enabled": false,
    "editor.wordWrap": "on" ,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.renderWhitespace": "all",
    "remote.SSH.connectTimeout": 90,
    "inlineChat.finishOnType": true,
    "chat.setupFromDialog": true,
    "github.copilot.enable": {
      "quarto": true,
      "*": true,
      "yaml": true
    },
    "editor.inlineSuggest.enabled": true,
    "terminal.integrated.profiles.linux":{ 
      "tmux workspace session": {
        "path": "tmux",
        "args": ["new-session", "-A", "-s", "vscode_${workspaceFolder}"]
      }
    },
    "github.copilot.advanced": {},
}